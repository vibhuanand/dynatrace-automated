{
  "name": "Innovizion - Core",
  "description": "Core management zone for Innovizion services",
  "rules": [
    {
      "type": "HOST",
      "enabled": true,
      "propagationTypes": ["PROCESS_GROUP_INSTANCE", "PROCESS_GROUP", "SERVICE"],
      "conditions": [
        {
          "key": { "attribute": "HOST_TAGS" },
          "comparisonInfo": {
            "type": "TAG",
            "operator": "EQUALS",
            "value": { "context": "CONTEXTLESS", "key": "innovizion" }
          }
        }
      ]
    }
  ]
}
